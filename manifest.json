{
    "name": "Chrome Dark Mode Everywhere",
    "version": "2.0",
    "description": "Produce dark mode on every chrome web pages!",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "declarativeContent",
        "scripting"
    ],
    "host_permissions": [
        "http://*/",
        "https://*/"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 3
}
